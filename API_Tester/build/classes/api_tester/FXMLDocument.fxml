<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="474.0" prefWidth="816.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="api_tester.FXMLDocumentController">
    <children>
      <Button fx:id="okBtn" layoutX="680.0" layoutY="412.0" mnemonicParsing="false" onAction="#handleOkbtn" prefHeight="34.0" prefWidth="114.0" text="Run">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
        <Button fx:id="chooserBtn" layoutX="100.0" layoutY="84.0" onAction="#handleChooserbtn" prefHeight="41.0" prefWidth="617.0" text="Upload File" textOverrun="CLIP">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font></Button>
      <Label fx:id="filePathLabel" layoutX="106.0" layoutY="136.0" prefHeight="34.0" prefWidth="602.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button layoutX="554.0" layoutY="412.0" mnemonicParsing="false" onAction="#handleCancelbtn" prefHeight="39.0" prefWidth="114.0" text="Cancel">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <TextArea fx:id="outputTextArea" layoutX="98.0" layoutY="181.0" prefHeight="200.0" prefWidth="618.0" />
    </children>
</AnchorPane>
